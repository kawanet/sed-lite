    "Yusuke Kawasaki <u-suke@kawa.net>"
  ],
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.1",
    "mocha": "^7.1.2",
    "typescript": "^3.9.2"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib/",
    "types/"
  ],
  "homepage": "https://github.com/kawanet/sed-lite#readme",
  "keywords": [
    "replace",
    "sed"
  ],
  "license": "MIT",
  "main": "lib/sed-lite.js",
  "repository": {
